<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>JavaWeb2</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>UsuarioController</display-name>
    <servlet-name>UsuarioController</servlet-name>
    <servlet-class>br.com.akira.controller.servlet.UsuarioController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UsuarioController</servlet-name>
    <url-pattern>/UsuarioController</url-pattern>
    <url-pattern>/usuController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UsuarioValidaLogin</display-name>
    <servlet-name>UsuarioValidaLogin</servlet-name>
    <servlet-class>br.com.akira.controller.servlet.UsuarioValidaLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UsuarioValidaLogin</servlet-name>
    <url-pattern>/UsuarioValidaLogin</url-pattern>
    <url-pattern>/usuValidaLogin</url-pattern>
  </servlet-mapping>
</web-app>